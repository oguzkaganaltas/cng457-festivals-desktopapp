      <ListView fx:id="festivalListView" layoutX="23.0" layoutY="79.0" prefHeight="180.0" prefWidth="230.0" AnchorPane.leftAnchor="15.0" />
      <ListView fx:id="concertListView" layoutX="268.0" layoutY="79.0" prefHeight="180.0" prefWidth="230.0" AnchorPane.leftAnchor="268.0" AnchorPane.rightAnchor="15.0" />
